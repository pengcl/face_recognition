{
  "name": "ng-alain-pro",
  "version": "0.0.0",
  "description": "Ng-alain business theme, ng-zorro-antd admin panel front-end framework",
  "author": "cipchk",
  "homepage": "https://e.ng-alain.com/theme/pro",
  "scripts": {
    "ng": "ng",
    "start": "npm run color-less && ng serve -o --disable-host-check --proxy-config proxy.conf.json --port 80",
    "hmr": "npm run color-less && ng serve -c=hmr",
    "build": "npm run color-less && ng build --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "icon": "ng g ng-alain:plugin icon"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@antv/data-set": "^0.10.1",
    "@antv/g2": "^3.4.8",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@auth0/angular-jwt": "^2.1.0",
    "@delon/abc": "^7.0.0",
    "@delon/acl": "^7.0.0",
    "@delon/auth": "^7.0.0",
    "@delon/cache": "^7.0.0",
    "@delon/chart": "^7.0.0",
    "@delon/form": "^7.0.0",
    "@delon/mock": "^7.0.0",
    "@delon/theme": "^7.0.0",
    "@delon/util": "^7.0.0",
    "@fullcalendar/core": "^4.0.0-beta.4",
    "@fullcalendar/daygrid": "^4.0.0-beta.4",
    "@fullcalendar/interaction": "^4.0.0-beta.4",
    "@fullcalendar/list": "^4.0.0-beta.4",
    "@fullcalendar/timegrid": "^4.0.0-beta.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^10.0.0",
    "ajv": "^6.9.2",
    "angular-baidu-maps": "^3.0.0",
    "core-js": "^2.5.4",
    "countup.js-angular2": "^7.1.1",
    "echarts": "^4.2.1",
    "element-closest": "^3.0.0",
    "file-saver": "^2.0.0",
    "js-base64": "^2.5.0",
    "masonry-layout": "^4.2.2",
    "ng-zorro-antd": "^7.0.0-rc.7",
    "ngx-countdown": "^3.2.0",
    "ngx-echarts": "^4.1.1",
    "ngx-image-gallery": "^1.3.1",
    "ngx-trend": "^3.4.1",
    "particles.js": "^2.0.0",
    "perfect-scrollbar": "^1.4.0",
    "qrious": "^4.0.2",
    "quill": "^1.3.6",
    "quill-image-resize-module": "^3.0.0",
    "rxjs": "~6.3.3",
    "screenfull": "^4.0.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@delon/testing": "^7.0.0",
    "@types/echarts": "^4.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-base64": "^2.3.1",
    "@types/jszip": "^3.1.5",
    "@types/mockjs": "^1.0.2",
    "@types/node": "~8.9.4",
    "codecov": "^3.2.0",
    "codelyzer": "~4.5.0",
    "editorconfig-tools": "^0.1.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^8.1.0",
    "mockjs": "^1.0.1-beta3",
    "ng-alain": "^7.0.0",
    "ng-alain-codelyzer": "^0.0.1",
    "prettier": "^1.16.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "xlsx": "^0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  },
  "private": true,
  "theme": {
    "name": "pro",
    "version": "7.1.2"
  }
}
